<?xml version="1.0" encoding="UTF-8"?>
    <addon id="repository.xanrepo" name="Xanax Repository" version="1.0" provider-name="Team Xanax">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="Xanax Repository">
            <info compressed="false">https://www.xanaxrepo.com/_repo/addons.xml</info>
            <checksum>https://www.xanaxrepo.com/_repo/addons.xml.md5</checksum>
            <datadir zip="true">https://www.xanaxrepo.com/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Repository for Xanax</summary>
            <description>This repository is the official home for the XANAX Wizard and Sportz APKs.</description>
            <platform>all</platform>
    </extension>
</addon>